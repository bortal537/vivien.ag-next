{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.4",
        "ext-json": "*",
        "ajcastro/eager-load-pivot-relations": "^0.2.2",
        "alfredo-ramos/parsedown-extra-laravel": "2.0.0",
        "anaseqal/nova-import": "^0.0.3",
        "beyondcode/laravel-dump-server": "^1.4",
        "beyondcode/laravel-self-diagnosis": "^1.4",
        "drobee/nova-sluggable": "^1.2",
        "epartment/nova-dependency-container": "1.2.*",
        "erusev/parsedown-extra": "0.8.1",
        "fideloper/proxy": "4.3.*",
        "fruitcake/laravel-cors": "2.0.*",
        "gijsg/system-resources": "1.0.5",
        "guzzlehttp/guzzle": "6.5.*",
        "idf/nova-html-card": "^1.2",
        "kabbouchi/nova-logs-tool": "0.2.*",
        "laravel/framework": "7.17.*",
        "laravel/nova": "3.7.*",
        "laravel/telescope": "3.5.*",
        "laravel/ui": "2.0.*",
        "league/flysystem-aws-s3-v3": "1.0.*",
        "livewire/livewire": "^1.0",
        "manogi/nova-tiptap": "0.6.*",
        "optimistdigital/nova-sortable": "^1.4",
        "saumini/count": "1.0.*",
        "sbine/route-viewer": "0.0.*",
        "spatie/emoji": "2.1.*",
        "spatie/laravel-activitylog": "3.14.*",
        "spatie/laravel-backup": "6.11.*",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-query-builder": "^2.8",
        "spatie/laravel-tags": "2.6.*",
        "spatie/nova-backup-tool": "4.0.*",
        "spatie/nova-tags-field": "3.0.*",
        "symfony/var-exporter": "5.1.*",
        "tightenco/nova-google-analytics": "^0.1.7",
        "titasgailius/search-relations": "^1.0",
        "vyuldashev/nova-money-field": "1.3.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.7.*",
        "beyondcode/laravel-query-detector": "^1.3",
        "beyondcode/tinkerwell-helper": "^1.1",
        "cloudstudio/resource-generator": "^1.5",
        "facade/ignition": "2.0.*",
        "fzaninotto/faker": "1.9.*",
        "krlove/eloquent-model-generator": "^1.3",
        "laracasts/generators": "^1.1",
        "laravel/tinker": "2.4.*",
        "mockery/mockery": "1.4.*",
        "nunomaduro/phpinsights": "1.14.*",
        "phpunit/phpunit": "9.2.*",
        "spatie/nova-tail-tool": "3.0.*",
        "supliu/laravel-query-monitor": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Systems\\Seed\\": "app/Systems/Seed/src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -W"
        ]
    },
    "repositories": {
        "laravel/nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
